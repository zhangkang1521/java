<form id="Colyn">
    <table cellpadding="0" cellspacing="0" id="rateTable" class="grid removeMinwidth gridBorder0">
        <tbody>
            <tr>
            <td colspan="6" class="lefts">
                <ul>
                    <li class="fl ml10 icon-bi">费用退回</li>
                    <li class="fr mr20"><a href="javascript:;" class="sjtop folding2"></a></li>
                </ul>
                <div class="clear"></div>
            </td>
        </tr>
         <tr>
                <td class="leftTD">收款人：</td>
                <td class="rightTD">
                    <span name="userName">$!refund.userName</span>
                </td>
          </tr>
           <tr>
                <td class="leftTD">收款人账号：</td>
                <td class="rightTD">
                    <span name="accountNo">$!refund.accountNo</span>
                </td>
           </tr>
             <tr>
                <td class="leftTD">手机号码：</td>
                <td class="rightTD">
                     <span name="userPhone">$!refund.userPhone</span>
                </td>
            </tr>
             <tr>
                <td class="leftTD">退回金额：</td>
                <td class="rightTD">
                   <span name="refundMoney">$!refund.refundMoney</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">退回原因：</td>
                <td class="rightTD">
                  <span name="refundReason">$!refund.refundReason</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">退费状态：</td>
                <td class="rightTD">
                  <span name="refundState">$!refund.refundState</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">审核意见：</td>
                    <td class="rightTD">
                  <span name="refundState">$!refund.applyReviewMessage</span>
                </td>
            </tr>
        </tbody>
    </table>
</form>
