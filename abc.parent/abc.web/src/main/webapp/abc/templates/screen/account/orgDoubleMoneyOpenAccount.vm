<div id="layout">
    <div region="center" title="钱多多平台企业开户" iconcls="icon-nav" class="regionCenter">
        <div id="wrap">
             <form id="Colyn">
                <table class="grid">
                 <tr>
                      <td class="leftTD" style="width: 45%">用户在网贷平台的账号：</td>
                        <td class="rightTD" colspan="2">
                            <span  class=" w170 easyui-validatebox" >$loginUserUtil.empName</span>
                            <input id="empName" type="hidden" value="$loginUserUtil.empName"/>
                            <input id="empId" type="hidden" value="$loginUserUtil.empId"/>
                        </td>
                    </tr>
                    <tr>
                        <td class="leftTD" style="width: 45%">企业名称：</td>
                        <td class="rightTD" colspan="2">
                           <input type="text" name="RealName" id="RealName" class="text-input"  readonly="readonly"/>
                        </td>
                    </tr>
                    <tr>
                      <td class="leftTD" style="width: 45%">营业执照号：</td>
                       <td class="rightTD" colspan="2">
                        <input type="text" name="IdentificationNo" id="IdentificationNo" class="text-input" readonly="readonly" />
                       </td>
                    </tr>
                    <tr>
                        <td class="leftTD" style="width: 45%">手机号：</td>
                        <td class="rightTD" colspan="2">
                            <input type="text" name="Mobile" id="Mobile" class="text-input" readonly="readonly" />
                        </td>
                    </tr>
                    <tr>
                        <td class="leftTD" style="width: 45%">公司邮箱：</td>
                        <td class="rightTD" colspan="2">
                            <input type="text" name="Email" id="Email" class="text-input" readonly="readonly"/>
                        </td>
                    </tr>                   
                    </table>
                    <input type="hidden" name="AccountMark" id="AccountMark">
                <div class="btn-div">
                    <a href="javascript:;" class="btns btns-primary" id="submit">开户</a>
                </div>
            </form>
        </div>
    </div>
</div>
﻿<script type="text/javascript" src="/Js/account/doubleMoneyOpenAccount.js"></script>
