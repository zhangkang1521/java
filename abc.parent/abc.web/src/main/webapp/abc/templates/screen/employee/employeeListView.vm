<div id="wrap">
<div id="layout">
    <div region="center" title="管理员维护" iconcls="icon-users" class="regionCenter">
        <div class="SearchForm hideClass" id="SearchForm">
            <form>
                <fieldset>
                    <legend class="ml10">信息查询</legend>
                    <div class="clearfloat search-wrap">
                        <ul class="fl search-width">
                            <li>
                                <span>用户名：</span>
                                <span>
                                    <input type="text" name="empName" id="empName" class="text-input ipt-w80" /></span>
                            </li>
                            <li>
                                <span>真实姓名：</span>
                                <span>
                                    <input type="text" name="empRealName" id="empRealName" class="text-input ipt-w80" /></span>
                            </li>
                            <li>
                                <span>状态：</span>
                                <span>
                                    <select class="select-input ipt-w94" name="empState" id="empState">
                                        <option value="all">全部</option>
                                        <option value="1">启用</option>
                                        <option value="0">停用</option>
                                    </select>
                                </span>
                            </li>
                        </ul>
                        <div class="fl search-div">
                            <a class="btns btns-primary wheight SearchHide" id="Search" href="#">搜索</a>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div id="toolbar">
            #showButton
        </div>
        <table id="AdminMaintainListGrid" toolbar="#toolbar" class="GridView regionTable"></table>
    </div>
</div>
﻿<script src="/Js/LinqJs/linq.min.js"  type="text/javascript" ></script>
<script src="/Js/LinqJs/linq.jquery.js"  type="text/javascript" ></script>
