<div id="layout">
    <div class="SearchForm hideClass">
        <form>
            <fieldset>
                <legend class="ml10">信息查询</legend>
                <div class="clearfloat search-wrap">
                    <ul class="fl search-width">
                        <li>
                            <span>机构名称：</span>
                            <span>
                                <input type="text" name="name" class="text-input ipt-w80" /></span>
                        </li>
                        <li>
                            <span>状态：</span>
                            <span>
                                <select class="select-input ipt-w94">
                                    <option>全部</option>
                                    <option>充值成功</option>
                                    <option>充值失败</option>
                                </select>

                            </span>
                        </li>
                        <li>
                            <span>充值日期：</span>
                            <span>
                                <input type="text" class="text-input easyui-datebox w95" name="start_add_date" id="" />
                                至
                                    <input type="text" class="text-input easyui-datebox w95" name="end_add_date" id="" />
                            </span>
                        </li>

                    </ul>
                    <div class="fl search-div">
                        <a id="search" class="btns btns-primary wheight " href="#">搜索</a>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
    <div id="toolbar">
     $control.setTemplate("common/showButton.vm")
    </div>
    <table id="GuaranteeAgenciesGrid" toolbar="#toolbar" class="GridView regionTable"></table>
</div>

<script type="text/javascript" src="/Js/recharge/GuarRechargeListView.js"></script>
