<form id="Colyn">
   
    <table cellpadding="0" cellspacing="0" class="grid removeMinwidth gridBorder0">
        <tbody>
            <tr>
                <td colspan="3" class="lefts">
                    <ul>
                        <li class="fl ml10 icon-bi">账户开户</li>
                        <li class="fr mr20"><a href="javascript:;" ></a></li>
                    </ul>
                    <div class="clear"></div>
                </td>
            </tr>
            <tr>
                <td class="leftTD">机构名称：</td>
                <td class="rightTD">
                    <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'LengthArea[2,20]'"  readonly="readonly" id="gov_guar_name" value="$!govAccount.govName" />
                    <input type="hidden" name="act_user_id" class="havechange" id="act_user_id" value="$!govAccount.accountUserId" />
                    <span id="spanguar_name" class="btns-dialog ml5 mt5 evt-AllGov" _duty="gov_guar_name|act_user_id"></span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">企业名称：</td>
                <td class="rightTD">
                     <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'LengthArea[2,20]'" id="act_user_name" name="act_user_name" value="$!govAccount.accountUserName" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">法人姓名：</td>
                <td class="rightTD">
                     <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'LengthArea[2,12]'" id="act_legal_name" name="act_legal_name" value="$!govAccount.accountLegalName" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">法人身份证号：</td>
                <td class="rightTD">
                    <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'IdCard'" name="act_user_card" value="$!govAccount.accountUserCard" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">开户账户：</td>
                <td class="rightTD" title="建议与手机号码保持一致！例：13288888888">
                     <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'Account'" id="act_account_no" name="act_account_no" value="$!govAccount.accountNo" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">法人手机号码：</td>
                <td class="rightTD" title="建议与开户账户保持一致！">
                     <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'Mobile'" name="act_user_phone" value="$!govAccount.accountUserPhone" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">法人邮箱地址：</td>
                <td class="rightTD">
                    <input type="text" class="text-input easyui-validatebox w170" data-options="validType:'Email'" name="act_user_email" value="$!govAccount.accountUserEmail" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">企业开户行地区：</td>
                <td class="rightTD" id="tdKhhdqdm">
                    <select id="act_bank_area_noA">
                    </select>
                    <select id="act_bank_area_noB" name="act_bank_area_no">
                    </select>
                    <input type="hidden" id="hdfact_bank_area_no" value="$!govAccount.accountBankArea" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">企业开户行行别：</td>
                <td class="rightTD" id="tdKhhhb">
                    <select id="act_bank_level" name="act_bank_level">
                    </select>
                    <input type="hidden" id="hdfact_bank_level" value="$!govAccount.accountBankName" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">企业开户行支行名称：</td>
                <td class="rightTD" id="tdKhhzhmc">
                     <input type="text" class="text-input easyui-validatebox w170" data-options="validType:'MaxLength[32]'" name="act_bank_name" value="$!govAccount.accountBankBranchName" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">企业对公账号：</td>
                <td class="rightTD" id="tdKhhyhzh">
                    <input type="text" class="text-input easyui-validatebox w170" data-options="required:true,validType:'LengthArea[10,19]'" name="act_user_account" value="$!govAccount.accountUserAccount" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">提现密码：</td>
                <td class="rightTD">
                     <input type="password" class="text-input easyui-validatebox w170" data-options="validType:'MaxLength[15]'" id="act_cash_pwdA" name="act_cash_pwd" value="$!govAccount.accountCashPwd" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">确认提现密码：</td>
                <td class="rightTD">
                    <input type="password" class="text-input easyui-validatebox w170" data-options="validType:'MaxLength[15]'" id="act_cash_pwdB" value="$!govAccount.accountCashPwd" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">备注：</td>
                <td class="rightTD">
                    <input type="text" class="text-input easyui-validatebox w170" data-options="validType:'MaxLength[20]'" id="act_account_mark" name="act_account_mark" value="$!govAccount.accountMark" />
                </td>
            </tr>
        </tbody>
    </table>
    <input type="hidden" name="act_user_type" class="havechange" value="3" />
    <input type="hidden" name="act_account_id" class="havechange" value="$!govAccount.accountId" />
    <input type="hidden" id="isAllowEdit" class="havechange" value="" />
</form>
