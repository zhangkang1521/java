<div id="layout">
    <div region="center" title="客户积分类型" iconcls="icon-users" class="regionCenter">
        <div class="SearchForm hideClass" id="SearchForm">
            <form>
                <fieldset>
                    <legend class="ml10">信息查询</legend>
                    <div class="clearfloat search-wrap">
                        <ul class="fl search-width">
                            <li>
                                <span>类型名称：</span>
                                <span>
                                    <input type="text" class="text-input ipt-w80" name ="scoreName" id ="scoreName" _method="Contains"/></span>
                            </li>
                            <li>
                                <span>类型代码：</span>
                                <span>
                                    <input type="text" class="text-input ipt-w80" name ="scoreCode" id ="scoreCode" _method="Contains"/></span>
                            </li>
                        </ul>
                        <div class="fl search-div">
                            <a class="btns btns-primary wheight SearchHide" href="#" id ="Search">搜索</a>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div id="toolbar">
            #showButton
        </div>
        <table id="IntegralTypesListGrid" toolbar="#toolbar" class="GridView regionTable"></table>
    </div>
</div>
<script type="text/javascript" src="/Js/score/ScoreTypeListView.js"></script>
