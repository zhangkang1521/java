<div id="wrap">
    <table cellpadding="0" cellspacing="0" id="rateTable" class="grid removeMinwidth gridBorder0">
        <tbody>
        <tr>
            <td colspan="6" class="lefts">
                <ul>
                    <li class="fl ml10 icon-bi">基本资料</li>
                    <li class="fr mr20"><a href="javascript:;" class="sjtop folding2"></a></li>
                </ul>
                <div class="clear"></div>
            </td>
        </tr>
        <tr>
            <td class="leftTD">客户名称：</td>
            <td class="rightTD">$!user.userName</td>
            <td class="leftTD">真实姓名：</td>
            <td class="rightTD">$!user.userRealName</td>
            <td class="leftTD">性别：</td>
            <td class="rightTD">$!user.userSex</td>
        </tr>
        <tr>
            <td class="leftTD">出生日期：</td>
            <td class="rightTD">
                $!user.userBirthday
            </td>
            <td class="leftTD">证件类型：</td>
            <td class="rightTD">$!user.userDocType</td>
            <td class="leftTD">证件号码：</td>
            <td class="rightTD">$!user.userDocNo</td>
        </tr>
        <tr>
            <td class="leftTD">电子邮箱：</td>
            <td class="rightTD">$!user.userEmail</td>
            <td class="leftTD">状态：</td>
            <td class="rightTD">$!user.userState</td>
            <td class="leftTD">籍贯：</td>
            <td class="rightTD">$!user.userNative</td>
        </tr>
        </tbody>

    </table>
    <input type="hidden" id="HiddenCinId" value="$!user.userId"/>
</div>
