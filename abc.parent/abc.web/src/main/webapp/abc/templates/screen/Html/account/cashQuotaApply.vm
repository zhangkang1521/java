<link href="/Css/Page.css" rel="stylesheet" type="text/css" />
<form id="applyForm" >
    <table cellpadding="0" cellspacing="0" class="grid removeMinwidth">
        <tr>
            <td class="leftTD">用户Id：</td>
            <td class="rightTD">
            <input type="text" class="text-input w130" id="act_user_id" name="cqaUserId"  readonly="readonly">
            </td>
        </tr>
        <tr>
            <td class="leftTD">客户名称：</td>
            <td class="rightTD">
            <input type="text" class="text-input w130" id="act_user_name" name="cqaUsername" readonly="readonly">
            </td>
        </tr>
        <tr>
            <td class="leftTD">原免费提现额度：</td>
            <td class="rightTD">
               <input type="text" class="text-input w130" id="act_user_cash" name="cqaOldCashQuota" readonly="readonly"/>
            </td>
        </tr>
        <tr>
            <td class="leftTD">申请增加提现额度：</td>
            <td class="rightTD">
               <input type="text" class="text-input w130 easyui-validatebox" id="cqaApplyCashQuota" name="cqaApplyCashQuota" validType="Moneyy" data-options="required:true"/>
            </td>
        </tr>
        <tr>
            <td class="leftTD">申请说明：</td>
            <td class="rightTD">
               <textarea type="text" class="text-input w130 easyui-validatebox" id="cqaInfo" name="cqaInfo" validType="MaxLength[80]" ></textarea>
            </td>
        </tr>
    </table>
</form>
