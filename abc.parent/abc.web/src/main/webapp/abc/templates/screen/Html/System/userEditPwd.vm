<link href="/Css/Page.css" rel="stylesheet" type="text/css" />
<form id="Colyn">
    <table cellpadding="0" cellspacing="0" class="grid removeMinwidth">      
        <tr>
            <td class="leftTD">新密码：</td>
            <td class="rightTD"><input type="password" id="emp_Password1" name="emp_Password" class="easyui-validatebox text-input" data-options="required:true,validType:'length[0,30]'" missingMessage ="请输入密码！"/></td>
        </tr>
        <tr>
            <td class="leftTD">确认密码：</td>
            <td class="rightTD"><input type="password" id="emp_Password2"  class="easyui-validatebox text-input" validType="EqualTo['#emp_Password1']"  data-options="required:true,missingMessage:'请再一次输入新密码'" missingMessage ="两次输入密码不匹配！"/></td>
        </tr>       
    </table>
</form>