<div id="wrap">
    <form id="Colyn">
        <table class="grid removeMinwidth" id="org">
            <tbody>
                <tr>
                    <td class="leftTD">用户名：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" data-options="required:true" name="" value="">
                    </td>
                    <td class="leftTD">真实姓名：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" name="" value=""> 
                    </td>
                    <td class="leftTD">性别：</td>
                    <td class="rightTD">
                        <input type="radio" name="1" id="" />男<input type="radio" name="1" id="" />女
                    </td>
                </tr>
                <tr>
                    <td class="leftTD">QQ：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" data-options="required:false,validType:'QQ'" name="" value="">
                    </td>
                    <td class="leftTD">出生日期：</td>
                    <td class="rightTD">
                        <input type="text" class="easyui-datebox w120 easyui-validatebox" data-options="required:false,validType:'Date'" name="" value="">
                    </td>
                    <td class="leftTD">电子邮箱：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" data-options="required:false,validType:'Email'" name="" value="">
                    </td>
                </tr>
                 <tr>
                    <td class="leftTD">联系方式：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" data-options="required:true" name="" value="">
                    </td>
                    <td class="leftTD">所在地：</td>
                    <td class="rightTD">
                        <select class="select-input ipt-w94">
                            <option>请选择省</option>
                            <option>安徽省</option>
                            <option>浙江省</option>
                            <option>湖北省</option>
                            <option>黑龙江省</option>
                        </select>
                        <select class="select-input ipt-w94">
                            <option>请选择市</option>
                        </select>
                         <select class="select-input ipt-w94">
                            <option>请选择县</option>
                        </select>
                    </td>
                    <td class="leftTD">详细地址：</td>
                    <td class="rightTD">
                        <input type="text" class="text-input w120 easyui-validatebox" data-options="required:true" name="" value=""> 
                    </td>
                </tr>
            </tbody>
        </table>
    </form>
</div>
