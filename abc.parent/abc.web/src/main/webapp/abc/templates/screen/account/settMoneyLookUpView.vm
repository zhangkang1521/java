
<script type="text/javascript" src="/Js/account/SettMoneyLookUpView.js"> </script>
<form id="Colyn">
   
    <table cellpadding="0" cellspacing="0" class="grid removeMinwidth gridBorder0">
        <tbody>
            <tr>
                <td colspan="3" class="lefts">
                    <ul>
                        <li class="fl ml10 icon-bi">余额查询</li>
                        <li class="fr mr20"><a href="javascript:;" class="sjtop folding2"></a></li>
                    </ul>
                    <div class="clear"></div>
                </td>
            </tr>
            <tr>
                <td class="leftTD">客户姓名：</td>
                <td class="rightTD">
                    $!userMoney.userName
                </td>
            </tr>
            <tr>
                <td class="leftTD">开户账户：</td>
                <td class="rightTD">
                    <span id="act_user_name">$!userMoney.userAccountNo</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">账面总余额：</td>
                <td class="rightTD">
                    <span id="spantotalBookBalance">$!userMoney.totalMoney</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">可用余额：</td>
                <td class="rightTD">
                    <span id="spanavailableBalance">$!userMoney.useableMoney</span>
                </td>
            </tr>
            <tr>
                <td class="leftTD">冻结余额：</td>
                <td class="rightTD">
                    <span id="spanamountFrozen">$!userMoney.frozenMoney</span>
                </td>
            </tr>
        </tbody>
    </table>
    <div class="btn-div"><a href="/account/settMoneyLookUpView" id="btnSerch"  class="btns btns-primary">刷新</a></div>

</form>

