<div id="layout">
    <div region="center" title="积分兑现划转" iconcls="icon-users" class="regionCenter">
        <div class="SearchForm hideClass" id="SearchForm">
            <form>
                <fieldset>
                    <legend class="ml10">信息查询</legend>
                    <div class="clearfloat search-wrap">
                        <ul class="fl search-width">
                            <li>
                                <span>兑现用户：</span>
                                <span>
                                    <input type="text" name="" class="text-input ipt-w95" /></span>
                                <span class="btns-dialog fl ml5"></span>
                            </li>
                            <li>
                                <span>兑现积分：</span>
                                <span>
                                    <input type="text" name="" class="text-input ipt-w95" /> 至 <input type="text" name="" class="text-input ipt-w95" /></span>
                            </li>
                            <li>
                                <span>兑现日期：</span>
                                <span>
                                    <input type="text" class="text-input easyui-datebox w95" name="add_date" id=""/>
                                    至
                                    <input type="text" class="text-input easyui-datebox w95" name="add_date" id="" />
                                </span>
                            </li>
                            <li>
                                <span>划转状态：</span>
                                <span>
                                    <select class="select-input ipt-w95">
                                        <option>全部</option>
                                        <option>待划转</option>
                                        <option>已划转</option>
                                    </select>
                                </span>
                            </li>
                        </ul>
                        <div class="fl search-div">
                            <a class="btns btns-primary wheight SearchHide" href="#">搜索</a>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div id="toolbar">
            @Html.Raw(ViewBag.Menus)
        </div>
        <table id="IntegralToCashListGrid" toolbar="#toolbar" class="GridView regionTable"></table>
    </div>
</div>
<script type="text/javascript" src="/Js/P2PMoneyManage/ScoreToCashTransferListView.js"></script>