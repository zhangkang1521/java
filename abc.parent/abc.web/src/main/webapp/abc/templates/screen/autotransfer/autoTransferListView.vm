﻿<div id="layout">
    <div region="center" title="自主转账" iconcls="icon-users" class="regionCenter">
     <div class="SearchForm hideClass" id="SearchForm">
            <form id="form1">
                <fieldset>
                    <legend class="ml10">信息查询</legend>
                    <div class="clearfloat search-wrap">
                        <ul class="fl search-width">
                            <li>
                                <span>付款人账户：</span>
                                <span>
                                    <input type="text" id="atPayAccotunt" name="atPayAccotunt"  _method="Contains" class="text-input ipt-w82" />
                                </span>
                            </li>
                            <li>
                                <span>收款人账户：</span>
                                <span>
                                    <input type="text" id="atReceibeAccotunt" name="atReceibeAccotunt" _method="Contains" class="text-input ipt-w82" />
                                </span>
                            </li>
                            <li>
                                <span>审核状态：</span>
                                <span>
                                    <select id="atAuditState" name="atAuditState" class="select-input ipt-w94">
                                        <option value="-1" selected>全部</option>
                                        <option value="0">待审核</option>
                                        <option value="1">已通过</option>
                                        <option value="2">未通过</option>
                                    </select>
                                </span>
                            </li>
                        </ul>
                        <div class="fl search-div">
                            <a id="SearchBtn" class="btns btns-primary wheight SearchHide " href="#">搜索</a>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div id="toolbar">
            #showButton
        </div>
        <table id="FriendLinkGrid" toolbar="#toolbar" class="GridView regionTable" ></table>
    </div>
</div>
<script type="text/javascript" src="/Js/auto/autoTransferListView.js"></script>
