<script type="text/javascript" src="/Js/moneyManage/MoneyReturnAddView.js"> </script>
<script type="text/javascript" src="/Js/P2PUC/UCCustomer.js"></script>

<form id="Colyn">
    <table cellpadding="0" cellspacing="0" id="rateTable" class="grid removeMinwidth gridBorder0">
        <tbody>
            <tr>
            <td colspan="6" class="lefts">
                <ul>
                    <li class="fl ml10 icon-bi">费用退回</li>
                    <li class="fr mr20"><a href="javascript:;" class="sjtop folding2"></a></li>
                </ul>
                <div class="clear"></div>
            </td>
        </tr>
            <tr>
                <td class="leftTD">收款人：</td>
                <td class="rightTD">
                    <input type="text" class="easyui-validatebox text-input w95" id="txt_rep_back_emp" data-options="required:true" missingmessage="请选择收款人！" />
                    <span class="btns-dialog ml5 mt5 evt-UserAccount" _duty="txt_rep_back_emp|hdf_rep_back_emp|rep_back_bank|rep_user_phone"></span>
                    <input type="hidden" id="hdf_rep_back_emp" name="rep_back_emp" value="0" />
                </td>
                <td class="leftTD">收款人账号：</td>
                <td class="rightTD">
                    <input type="text" id="rep_back_bank" name="rep_back_bank" class="easyui-validatebox text-input w95" data-options="required:true" />
                </td>
                <td class="leftTD">手机号码：</td>
                <td class="rightTD">
                    <input id="rep_user_phone" name="rep_user_phone" type="text" class="easyui-validatebox text-input w95" data-options="required:true" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">退回金额：</td>
                <td class="rightTD" colspan="5">
                    <input id="rep_return_amount" name="rep_return_amount" type="text" class="easyui-validatebox text-input w95" data-options="required:true" missingmessage="请输入退回金额！" />
                </td>
            </tr>
            <tr>
                <td class="leftTD">退回原因：</td>
                 <td class="rightTD" colspan="5">
                     <textarea id="rep_reject_reason" name="rep_reject_reason" class="textarea-text w80s cli_blu grayleft mt5 mb5 evt-keyup" _length="60" _value="请输入..."></textarea>
                     <span class="showText noticeTD">可以输入60字</span>
                  </td>
                </tr>
        </tbody>
      
    </table>
    <div class="btn-div">
         <a href="#" id="returnMoneyBtn" class="btns btns-primary ">退费申请</a>
    </div>
</form>
