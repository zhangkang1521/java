@model Auto.Model.SYS_template_config
<form id="Colyn">

    <table cellpadding="0" cellspacing="0" class="grid removeMinwidth gridBorder0">
        <input type="hidden" name="sys_template_id" value="@Model.sys_template_id" />
        <tbody>
            <tr>
                <td colspan="3" class="lefts">
                    <ul>
                        <li class="fl ml10 icon-bi">编号模板设置</li>
                        <li class="fr mr20"><a href="javascript:;" class="sjtop folding2"></a></li>
                    </ul>
                    <div class="clear"></div>
                </td>
            </tr>
            <tr>
                <td class="leftTD w170">项目名称模板：</td>
                <td class="rightTD w280">
                    <input type="text" class="text-input" name="sys_template_no" value="@Model.sys_template_no"></td>
                <td class="leftTD w280"><span style="color: Red; float: left" id="sys_template_no"></span></td>
            </tr>
        </tbody>

    </table>
    <div class="divYellow">
        <p class="noticeTD">注：  $year$是年，$month$是月， $day$ 是日 $000n$是编号</p>
    </div>


    <div class="btn-div"><a href="#" id="btnUpdate" class="btns btns-primary">保存模板</a></div>
</form>
<script type="text/javascript" src="/Js/P2PSystemManage/TemplateSettingAddView.js"></script>
