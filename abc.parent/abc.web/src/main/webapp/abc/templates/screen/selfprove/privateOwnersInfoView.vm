                <td class="leftTD">按揭银行：</td>
                <td class="rightTD">$!userOwner.uoBankPay</td>
            </tr>
        </tbody>
      
    </table>
        <input type="hidden"id="HiddenCinId" value ="$!userOwner.uoId"/>
</div>
